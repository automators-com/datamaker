{
  "name": "datamaker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|tsx|ts|json)\"",
    "migrate:dev": "dotenv -e .env.local -- pnpx prisma migrate dev",
    "studio": "dotenv -e .env.local -- pnpx prisma studio",
    "preinstall": "npx -y only-allow pnpm",
    "postinstall": "pnpx prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@premieroctet/next-crud": "^2.3.1",
    "@prisma/client": "^4.6.1",
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query": "^4.19.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "bcryptjs": "^2.4.3",
    "eslint-config-next": "13.0.2",
    "next": "13.0.2",
    "next-auth": "^4.18.6",
    "prisma": "^4.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.7",
    "react-spinners": "^0.13.7",
    "react-tooltip": "^4.5.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.2"
  }
}
